@model List<KategoriModel>
<h2>Kategoriler</h2>

<table class="table">
    <thead>
        <tr>
        <th>
            Adı
        </th>
        <th>
            Açıklaması
        </th>
        </tr>
    </thead>
    <tbody>
        @foreach (KategoriModel kategori in Model)
        {
            <tr>
                <td>
                    @kategori.Adi
                </td>
                <td>
                    @kategori.Aciklamasi
                </td>
            </tr>
        }
    </tbody>
</table>